{
  "name": "node-dtls-client",
  "version": "0.6.0",
  "description": "DTLS protocol implementation for Node.js written in TypeScript.",
  "homepage": "https://github.com/AlCalzone/node-dtls-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlCalzone/node-dtls-client.git"
  },
  "license": "MIT",
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc",
    "ci": "yarn type && yarn lint && yarn test",
    "fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "type": "tsc --noEmit",
    "unused": "organize-imports-cli src/**/*.ts"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "preset": "ts-jest",
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "dependencies": {
    "binary-data": "^0.6.0",
    "debug": "^4.1.1",
    "node-forge": "^0.9.1",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^1.2.0",
    "@types/debug": "4.1.5",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.1.1",
    "@types/node-forge": "^0.9.3",
    "@types/semver": "^7.1.0",
    "@types/sinon": "^9.0.0",
    "@types/yargs": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "colors": "^1.2.3",
    "coveralls": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.3.0",
    "nyc": "^15.0.0",
    "organize-imports-cli": "^0.7.0",
    "prettier": "^2.0.4",
    "proxyquire": "^2.0.1",
    "rimraf": "^3.0.0",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.0.0",
    "source-map-support": "^0.5.5",
    "ts-jest": "^25.3.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.8.3",
    "yargs": "^15.0.1"
  },
  "engines": {
    "node": ">=6"
  },
  "readme": "README.md"
}
